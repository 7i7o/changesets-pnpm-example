{
    "name": "@7i7o/changesets-pnpm-example",
    "version": "1.0.0",
    "description": "Example repo to show how can changesets be used to publish npm packages",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "scripts": {
        "lint": "tsc",
        "build": "tsup src/index.ts --format cjs,esm --dts",
        "release": "pnpm run build && changeset publish"
    },
    "keywords": [
        "typescript",
        "pnpm",
        "tsup",
        "changesets"
    ],
    "author": "7i7o",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist/",
        "src/",
        ".github/",
        ".gitignore",
        "package.json",
        "tsconfig.json",
        "README.md",
        "CHANGELOG.md"
    ],
    "homepage": "https://github.com/7i7o/changesets-pnpm-example#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/7i7o/changesets-pnpm-example.git"
    },
    "devDependencies": {
        "@changesets/cli": "^2.27.1",
        "tsup": "^8.0.1",
        "typescript": "^5.3.3"
    }
}
